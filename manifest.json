{
    "name": "OneID Form Filler",
    "version": "1.0",
    "manifest_version": 2,
    "description": "Populate forms from any website.",
    "background":{
        "scripts" : ["js/background.js"]
    },
    "browser_action": {
        "default_icon": "images/icon.png"
    },
    "permissions" : [
        "*://*/*"
    ],
    "web_accessible_resources": [
        "images/id-big-logo.png"
    ]
}
