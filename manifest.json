{
    "name": "OneID Form Filler",
    "version": "1.0",
    "manifest_version": 2,
    "description": "Populate forms from any website.",
    "background":{
        "scripts" : ["js/main.js"]
    },
    "browser_action": {
        "default_icon": "icon.png"
    },
    "permissions" : [
        "*://*/*"
    ]
}
